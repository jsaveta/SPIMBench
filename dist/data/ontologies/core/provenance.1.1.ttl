# Saved by TopBraid on Fri Mar 15 16:52:39 GMT 2013
# baseURI: http://www.bbc.co.uk/ontologies/provenance/
# imports: http://www.bbc.co.uk/ontologies/bbc/

@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix provenance:  <http://www.bbc.co.uk/ontologies/provenance/> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

provenance:
      rdf:type owl:Ontology ;
      rdfs:comment "An ontology to capture the classes and relationships of systems and applications. Data from this ontology is not intended to be exposed outside the BBC and it's used mainly data management and by the associated APIs"^^xsd:string ;
      rdfs:label "Provenance Ontology"^^xsd:string ;
      dc:created "$Date: 2012/11/21 15:39:23 $"^^xsd:string ;
      owl:imports <http://www.bbc.co.uk/ontologies/bbc/> ;
      owl:versionInfo "1.1"^^xsd:string .

provenance:Dataset
      rdf:type owl:Class ;
      rdfs:comment "The class to represent a named graph where all the triples contained represent a collection of data describing some domain or sub-domain "^^xsd:string ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "Dataset"^^xsd:string ;
      rdfs:subClassOf provenance:Graph .

provenance:Graph
      rdf:type owl:Class ;
      rdfs:comment "A graph of triples of common provenance"^^xsd:string ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "Graph"^^xsd:string ;
      rdfs:subClassOf owl:Thing .

provenance:Ontology
      rdf:type owl:Class ;
      rdfs:comment " The class to represent a named graph which is an ontology"^^xsd:string ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "Ontology"^^xsd:string ;
      rdfs:subClassOf provenance:Graph .

provenance:Resource
      rdf:type owl:Class ;
      rdfs:comment " The class to represent a named graph where all the triples contained represent some kind of atomic resource (e.g., a particular thing like the URI for Usain Bolt)"^^xsd:string ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "Resource"^^xsd:string ;
      rdfs:subClassOf provenance:Graph .

provenance:canonicalLocation
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "The canonical location where all the triples contained within the context (named graph) are stored externally. The location should be parametised with the version value to represent the different available versions. At the BBC we will use SVN locations."^^xsd:string ;
      rdfs:domain provenance:Graph ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "canonicalLocation"^^xsd:string ;
      rdfs:range xsd:string .

provenance:changeReason
      rdf:type owl:DatatypeProperty ;
      rdfs:comment " The reason for the most recent change to the data in the context/named graph "^^xsd:string ;
      rdfs:domain provenance:Graph ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "changeReason"^^xsd:string ;
      rdfs:range xsd:string .

provenance:deprecated
      rdf:type owl:DatatypeProperty ;
      rdfs:comment " Whether or not the dataset has been deprecated. This property is added automatically when empty datasets are provided. It may also be used for later 'cleanups' of deprecated datasets. "^^xsd:string ;
      rdfs:domain provenance:Dataset ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "deprecated"^^xsd:string ;
      rdfs:range xsd:boolean .

provenance:owner
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "A reference to the person or system who owns the data. URIs can be used to represent people, or alternatively a string can be used to provide email addresses or other identifiers. In a CMS example, this would indicate the CMS user and not the CMS itself. "^^xsd:string ;
      rdfs:domain provenance:Graph ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "provider"^^xsd:string ;
      rdfs:range xsd:anyURI .

provenance:previousVersionHash
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "This is needed in order to perform rollbacks and to ensure that the data is consistent among the environment. The hash of the previous version must match the hash of generated by the previous version of the dataset"^^xsd:string ;
      rdfs:domain provenance:Graph ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "previousVersionHash"^^xsd:string ;
      rdfs:range xsd:string .

provenance:product
      rdf:type owl:ObjectProperty ;
      rdfs:comment "The reference to the product from which the dataset, graph was created. This is needed for dataset management permissions"^^xsd:string ;
      rdfs:domain provenance:Graph ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "product"^^xsd:string ;
      rdfs:range <http://www.bbc.co.uk/ontologies/bbc/Product> .

provenance:provided
      rdf:type owl:DatatypeProperty ;
      rdfs:comment " The date and time at which the data was provided to the store "^^xsd:string ;
      rdfs:domain provenance:Graph ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "provided"^^xsd:string ;
      rdfs:range xsd:dateTime .

provenance:provider
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "A reference to the person or system who uploaded the dataset. URIs can be used to represent people, or alternatively a string can be used to provide email addresses or other identifiers. In a CMS example, this would indicate the CMS user and not the CMS itself. "^^xsd:string ;
      rdfs:domain provenance:Graph ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "provider"^^xsd:string ;
      rdfs:range xsd:anyURI .

provenance:version
      rdf:type owl:DatatypeProperty ;
      rdfs:comment " The version of the data. On creation, the version will be 1, subsequent versions will increment the version number. Only integer versions are supported. "^^xsd:string ;
      rdfs:domain provenance:Graph ;
      rdfs:isDefinedBy provenance: ;
      rdfs:label "version"^^xsd:string ;
      rdfs:range xsd:int .
